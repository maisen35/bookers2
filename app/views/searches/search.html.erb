<div class="container">
  <div class="row">
    <div class="col">
      <% if "User" == @range %>
        <table>
          <tr>
            <th>ユーザ</th>
            <th></th>
          </tr>
          <% @users.each do |user| %>
            <tr>
              <td><%= user.name %></td>
              <td></td>
            </tr>
          <% end %>
        </table>
      <% else %>
        <table>
          <tr>
            <th>タイトル</th>
            <th></th>
          </tr>
          <% @books.each do |book| %>
            <tr>
              <td><%= book.title %></td>
              <td></td>
            </tr>
          <% end %>
        </table>
      <% end %>
    </div>
  </div>
</div>